<body class="">
  <div class="wrapper ">
    <div class="sidebar" data-color="green" data-background-color="black" data-image="../assets/img/agri.jpg">
      <!--
        Tip 1: You can change the color of the sidebar using: data-color="purple | azure | green | orange | danger"

        Tip 2: you can also add an image using data-image tag
    -->
      <div class="logo">
        <a class="simple-text logo-mini" >
          
        </a>
        
        <a href="rainfall.html">
          <img src="../assets/img/logo.png" alt="AGRO PRO" height="70"style="padding-left:6%">
        </a>
      </div>
      <div class="sidebar-wrapper">
        <br>
        <ul class="nav">
          <li class="nav-item">
            <a class="nav-link" href="rainfall.html">
              <i class="material-icons">opacity</i>
              <p style="font-size: 130%;"> Rainfall Prediction</p>
            </a>
          </li>
          <li class="nav-item ">
            <a class="nav-link" href="crop.html">
              <i class="material-icons">eco</i>
              <p style="font-size: 130%;"> Crop Prediction </p>
            </a>
          </li>
          <li class="nav-item active">
            <a class="nav-link" href="news.html">
              <i class="material-icons">bubble_chart</i>
              <p style="font-size: 130%;"> News Feed </p>
            </a>
          </li>
          <li class="nav-item ">
            <a class="nav-link" href="schemes.html">
              <i class="material-icons">notifications</i>
              <p style="font-size: 130%;"> Government Schemes</p>
            </a>
          </li>
        </ul>
      </div>
    </div>
    <div class="main-panel">
      <nav class="navbar navbar-expand-lg navbar-transparent navbar-absolute fixed-top ">
        <div class="container-fluid">
          <div class="navbar-wrapper">

          </div>
          <button class="navbar-toggler" type="button" data-toggle="collapse" aria-controls="navigation-index" aria-expanded="false" aria-label="Toggle navigation">
            <span class="sr-only">Toggle navigation</span>
            <span class="navbar-toggler-icon icon-bar"></span>
            <span class="navbar-toggler-icon icon-bar"></span>
            <span class="navbar-toggler-icon icon-bar"></span>
          </button>
          
        </div>
      </nav>
      <div class="card" style="margin-top: 0%; margin-bottom: 0;">
        <div class="card-header" style="margin-bottom: 0;margin-top: 4%;">
          <h3 class="card-title"><b>News Feed</b></h3>
          <br>
        </div>
        <div class = "row">
          <form action="/news.html" method="GET" style="padding-left: 30px;">
            <button type="submit" class="btn btn-<%=a%> " style="border-radius: 20px;">Rainfall</button>
          </form>
          <form action="/agriculture" method="GET" style="padding-left: 10px;">
            <button type="submit" class="btn btn-<%=b%>" style="border-radius: 20px;">Agriculture</button>
          </form>
          <form action="/farmers" method="GET" style="padding-left: 10px;"> 
            <button type="submit" class="btn btn-<%=c%>" style="border-radius: 20px;">Farmers</button>
          </form>
          <form action="/irrigation" method="GET" style="padding-left: 10px;">
            <button type="submit" class="btn btn-<%=d%>" style="border-radius: 20px;">Irrigation</button>
          </form>
        </div>
      <br>
      <h3 style="padding-left: 20px;"> Latest news on <%= x %></h3>
      <br>
      <div class="row">
      <% data.forEach(function(r) { %>
          <div class="col-md-6">
            <div class="card card-product">
              <div class="card-header card-header-image">
                  <img class="img" src="<%= r.urlToImage %>" style="height:250px;">
              </div>
              <div class="card-body">
                <h4 class="card-title">
                  <a href="<%= r.url %>" onMouseOver="this.style.color='green'" onMouseOut="this.style.color='black'" style="color: black;"><%= r.title %></a>
                </h4>
                <div class="card-description">
                  <%=r.content %>
                </div>
              </div>
              <div class="card-footer">
                <div class="stats">
                  <h6>Date of Publish: <%= r.publishedAt.slice(0,10)%></h6>
                </div>
                <div class="stats">
                  <h6 class="card-category">Autor: <%= r.author %></h6>
                </div>
              </div>
            </div>
          </div>
        
       <% });%>
      </div>
    </div>
    </div>
      
